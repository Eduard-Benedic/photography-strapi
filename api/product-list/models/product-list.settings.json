{
  "kind": "collectionType",
  "collectionName": "product_lists",
  "info": {
    "name": "ProductList"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "typeStripeIdentifier": {
      "type": "enumeration",
      "enum": [
        "fashionAndBeauty",
        "ecommerce",
        "socialMedia"
      ],
      "required": true
    },
    "subtypeStripeIdentifier": {
      "type": "enumeration",
      "enum": [
        "ghostMannequin",
        "onModel",
        "packshot",
        "lifestyle",
        "whiteBackground",
        "flat"
      ],
      "required": true
    },
    "productImage": {
      "type": "component",
      "repeatable": true,
      "component": "reusable-components.stripe-strapi-identifier",
      "required": true,
      "min": 4,
      "max": 4
    },
    "additionalShot": {
      "type": "component",
      "repeatable": true,
      "component": "reusable-components.additional-shot",
      "required": true
    }
  }
}
